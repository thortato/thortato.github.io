 } from \"react\";\r\nimport '../Components/Sidebar.css';\r\nimport { House, AddressBook,List, X ,CirclesFour, PaperPlaneTilt} from \"phosphor-react\";\r\n\r\nexport default function SideBar(props) {\r\n    const [isCollapsed, setIsCollapsed] = useState(true);\r\n\r\n    const toggleSidebar = () => {\r\n    setIsCollapsed(!isCollapsed);\r\n    }\r\n\r\n    return (\r\n        <div>    \r\n            <div className={`sidebar ${isCollapsed ? 'collapse' : 'toggle'}`}>\r\n                <h3 className='subtitle'>MENU \r\n                <button onClick={toggleSidebar} className=\"closeButton\"><X size={32} /></button></h3>\r\n                <a href={''} className='option'><House size={32} className=\"buttonAlign\"/>Home</a>\r\n                <a href={''} className='option'><AddressBook size={32} />About</a>\r\n                <a href={''} className='option'><CirclesFour size={32} />Works</a>\r\n                <a href={''} className='option'><PaperPlaneTilt size={32} />Contact Me</a>\r\n            </div>\r\n            <button onClick={toggleSidebar} className=\"toggleButton\"><List size={32} /></button>\r\n        </div>\r\n    );\r\n\r\n};\r\n","C:\\Users\\ASUS\\OneDrive\\Documents\\todolist\\src\\ScrolltoTop.js",[],[],"C:\\Users\\ASUS\\OneDrive\\Documents\\todolist\\src\\Pages\\Works\\Work_Alterland.js",["97"],[],"import \"./../../Components/Content.css\";\r\nimport \"./works.css\";\r\nimport { Link } from \"react-router-dom\";\r\nimport HeroIllustration from \"../../Assets/Image/Hero_Alterland.png\";\r\nimport MayorsIllustration from \"./../../Assets/Image/Alterland_Asset/Mayors.png\";\r\nimport LevelIllustration from \"./../../Assets/Image/Alterland_Asset/PowerLv1_Lv4.jpg\";\r\nimport ComparisonIllustration from \"./../../Assets/Image/Alterland_Asset/AlterlandComparison.jpg\";\r\nimport { HomeButton } from \"../../Components/HomeButton\";\r\n\r\nexport function WorkAlterland() {\r\n  return (\r\n    \r\n    <div>\r\n      <HomeButton/>\r\n        <div className=\"relative\">\r\n      {/* Image */}\r\n      <img\r\n        className=\"brightness-50 w-full md:h-96\"\r\n        src={HeroIllustration}\r\n        alt=\"Hero Illustration\"\r\n      />\r\n\r\n      {/* Text */}\r\n      <div className=\"absolute top-8 md:top-32 left-0 right-0 text-center white-text font-bold\">\r\n        <h1>Alterland</h1>\r\n        <p>City Building Game</p>\r\n        <p>January 2022 - May 2022</p>\r\n      </div>\r\n    </div>\r\n\r\n      <div className=\"flex flex-col gap-10 px-5 md:px-[400px] mt-10\">\r\n        <div>\r\n          <p>\r\n            At the start of 2022, the popularity of NFT and crypto based game\r\n            have skyrocketed like never before. My then agency, Renderkin Studio\r\n            decided to follow the trend to explore what the technology can do.\r\n            And thus, the concept of Alterland was born..\r\n          </p>\r\n        </div>\r\n\r\n        <div>\r\n          <h1>My Role</h1>\r\n          <p>\r\n            As a 3D general artist, i am tasked to conceptualize and design the\r\n            overall look of the game with the supervision of my then manager.\r\n          </p>\r\n          <p>\r\n            The process of designing the look and feel of the game starts with:\r\n          </p>\r\n        </div>\r\n        <div>\r\n          <h2 className=\"text-3xl font-bold md:mb-14\">\r\n            <span className=\"highlight\">Research</span>\r\n          </h2>\r\n          <p>\r\n            To start with, we surf through the web to find then ongoing\r\n            blockchain and NFT affiliated games. One of the more prolific game\r\n            at the time was Sandbox, with its voxel based art direction.\r\n            However, there are quite a lot of these voxel like aesthetics at the\r\n            time to which the studio decided to not use it as to distinct\r\n            Alterland from its competitors.\r\n          </p>\r\n          <p>\r\n            We then decided to turn to Pinterest to look for further insight.\r\n            During the search we found that low-poly isometric style may just\r\n            works for Alterland. So the studio set the artstyle of Alterland to\r\n            be that of low-poly isometric style.\r\n          </p>\r\n        </div>\r\n        <div>\r\n          <h2 className=\"text-3xl font-bold md:mb-14\">\r\n            <span className=\"highlight\">Modelling</span>\r\n          </h2>\r\n          <p>\r\n            Alterland is a city building game where player can create and manage\r\n            their own land. There are 14 types of building that player can build\r\n            with each building type having their own benefit to help the player.\r\n            Most building type can be levelled up to improve its function and\r\n            changes its sprite, upward to level 4.\r\n          </p>\r\n          <img src={LevelIllustration} alt=\"Level\" className=\"p-5\" />\r\n          <p>\r\n            In Alterland, the 3D assets are not ported directly into the game\r\n            engine but the rendered 2D sprites was used instead. It resulted in\r\n            each asset needed to be rendered 4 times for each angle to\r\n            compensate for the isometric style used.\r\n          </p>\r\n        </div>\r\n        <div>\r\n          <h2 className=\"text-3xl font-bold md:mb-14\">\r\n            <span className=\"highlight\">Iterate</span>\r\n          </h2>\r\n          <p>\r\n            While the reference found in Pinterest was fine for the earlier\r\n            level building, things started to get complicated when designing the\r\n            higher levelled building. Higher levelled building were designed to\r\n            look more complex to make the player feel the progression they made\r\n            when upgrading buildings.\r\n          </p>\r\n          <p>\r\n            These complexity of course, runs against the aforementioned low-poly\r\n            style. We decided to use SimCity as example for higher levelled\r\n            building while slightly compensating the complexity for a more\r\n            casual low-poly style.\r\n          </p>\r\n          <img src={ComparisonIllustration} alt=\"Comparison\" className=\"p-5\" />\r\n        </div>\r\n        <div>\r\n          <h2 className=\"text-3xl font-bold md:mb-14\">\r\n            <span className=\"highlight\">Result</span>\r\n          </h2>\r\n          <p>\r\n            Over the course of three months, I created approximately 65 assets\r\n            for the game, each crafted with shading, texturing and lighting\r\n            effects. Additionally, I've also created six character portraits\r\n            intended for in-game uses.\r\n          </p>\r\n          <p>\r\n            However, as the hype surrounding NFTs and crypto dwindled down, this\r\n            project was sadly suspended indefinitely. Nevertheless, as my first\r\n            project I am grateful for the experience gained and the opportunity\r\n            to enhance my 3D skills through my involvement in the Alterland\r\n            project.\r\n          </p>\r\n          <img src={MayorsIllustration} alt=\"Mayor\" className=\"p-5 mb-20\" />\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n","C:\\Users\\ASUS\\OneDrive\\Documents\\todolist\\src\\Pages\\Works\\Work_IntotheForest.js",[],[],"C:\\Users\\ASUS\\OneDrive\\Documents\\todolist\\src\\Pages\\Home.js",[],[],"C:\\Users\\ASUS\\OneDrive\\Documents\\todolist\\src\\Pages\\Footer.js",[],[],"C:\\Users\\ASUS\\OneDrive\\Documents\\todolist\\src\\Pages\\Works.js",[],[],"C:\\Users\\ASUS\\OneDrive\\Documents\\todolist\\src\\Pages\\HeroSection.js",[],[],"C:\\Users\\ASUS\\OneDrive\\Documents\\todolist\\src\\Pages\\About.js",[],[],"C:\\Users\\ASUS\\OneDrive\\Documents\\todolist\\src\\Pages\\ContactMe.js",["98"],[],"import '../Components/Content.css';\r\nimport ContactMeIllustration from './../Assets/Image/ContactMeIllustration.png';\r\n\r\nexport default function ContactMe(){\r\n\r\n    return(\r\n        <div className='contactMe'>\r\n            <div className='text-center text-3xl md:text-5xl font-black z-20'>Interested in my works? Say hello to me here!</div>\r\n            <a href=\"mailto:jvcrotanson@gmail.com\" className='z-20'><button className='contactMe-button'>Hello!</button></a>\r\n            \r\n        </div>\r\n    \r\n    );\r\n        \r\n    \r\n};","C:\\Users\\ASUS\\OneDrive\\Documents\\todolist\\src\\Components\\Card.js",[],[],"C:\\Users\\ASUS\\OneDrive\\Documents\\todolist\\src\\Components\\HeroImage.js",[],[],"C:\\Users\\ASUS\\OneDrive\\Documents\\todolist\\src\\Components\\HomeButton.js",[],[],"C:\\Users\\ASUS\\OneDrive\\Documents\\todolist\\src\\Pages\\Works\\Work_PenyelamatBumi.js",["99"],[],"import \"./../../Components/Content.css\";\r\nimport \"./works.css\";\r\nimport { Link } from \"react-router-dom\";\r\nimport HeroIllustration from \"../../Assets/Image/Hero_PenyelamatBumi.png\";\r\nimport EnemiesIllustration from \"./../../Assets/Image/PB_Monsters.png\";\r\nimport LevelIllustration from \"./../../Assets/Image/Alterland_Asset/PowerLv1_Lv4.jpg\";\r\nimport ComparisonIllustration from \"./../../Assets/Image/Alterland_Asset/AlterlandComparison.jpg\";\r\nimport { HomeButton } from \"../../Components/HomeButton\";\r\n\r\nexport function WorkPenyelamatBumi() {\r\n  return (\r\n    <div>\r\n      <HomeButton/>\r\n      <div className=\"relative\">\r\n        {/* Image */}\r\n        <img\r\n          className=\"brightness-50 w-full md:h-96\"\r\n          src={HeroIllustration}\r\n          alt=\"Hero Illustration\"\r\n        />\r\n\r\n        {/* Text */}\r\n        <div className=\"absolute top-8 md:top-32 left-0 right-0 text-center white-text font-bold\">\r\n          <h1>Penyelamat Bumi</h1>\r\n          <p>Environmental Friendly 3D Platform Game</p>\r\n          <p>July 2020 - October 2020</p>\r\n        </div>\r\n      </div>\r\n\r\n      <div className=\"flex flex-col gap-10 px-5 md:px-[400px] mt-10\">\r\n        <div>\r\n          <p>\r\n            GEMASTIK or otherwise known as Pagelaran Mahasiswa Nasional Bidang\r\n            Teknologi Informasi Dan Komunikasi is a competition aimed to improve\r\n            and guide students across Indonesia so that they will be able to\r\n            lead a change in the national's tech sector. Each team consists of\r\n            three participants, with each competing on different section. Me and\r\n            my friend at the time decided to join the game development section\r\n          </p>\r\n        </div>\r\n\r\n        <div>\r\n          <h1>My Role</h1>\r\n          <p>\r\n            As the artist of the team, my role involves me directing the general\r\n            art direction and assets making such as the enemy unit, UI. and environment arrangement.\r\n          </p>\r\n          <p>Below are some of the things I've worked on:</p>\r\n        </div>\r\n        <div>\r\n          <h2 className=\"text-3xl font-bold md:mb-14\">\r\n            <span className=\"highlight\">Enemies</span>\r\n          </h2>\r\n          <img src={EnemiesIllustration} alt=\"Enemies\" className=\"p-5 mb-20\" />\r\n          <p>\r\n            One of the points that we are trying to make is to teach younger audience to keep environment clean, such as throwing trash in the right way\r\n            To start with, we surf through the web to find then ongoing\r\n            blockchain and NFT affiliated games. One of the more prolific game\r\n            at the time was Sandbox, with its voxel based art direction.\r\n            However, there are quite a lot of these voxel like aesthetics at the\r\n            time to which the studio decided to not use it as to distinct\r\n            Alterland from its competitors.\r\n          </p>\r\n          <p>\r\n            We then decided to turn to Pinterest to look for further insight.\r\n            During the search we found that low-poly isometric style may just\r\n            works for Alterland. So the studio set the artstyle of Alterland to\r\n            be that of low-poly isometric style.\r\n          </p>\r\n        </div>\r\n        <div>\r\n          <h2 className=\"text-3xl font-bold md:mb-14\">\r\n            <span className=\"highlight\">Modelling</span>\r\n          </h2>\r\n          <p>\r\n            Alterland is a city building game where player can create and manage\r\n            their own land. There are 14 types of building that player can build\r\n            with each building type having their own benefit to help the player.\r\n            Most building type can be levelled up to improve its function and\r\n            changes its sprite, upward to level 4.\r\n          </p>\r\n          <img src={LevelIllustration} alt=\"Level\" className=\"p-5\" />\r\n          <p>\r\n            In Alterland, the 3D assets are not ported directly into the game\r\n            engine but the rendered 2D sprites was used instead. It resulted in\r\n            each asset needed to be rendered 4 times for each angle to\r\n            compensate for the isometric style used.\r\n          </p>\r\n        </div>\r\n        <div>\r\n          <h2 className=\"text-3xl font-bold md:mb-14\">\r\n            <span className=\"highlight\">Iterate</span>\r\n          </h2>\r\n          <p>\r\n            While the reference found in Pinterest was fine for the earlier\r\n            level building, things started to get complicated when designing the\r\n            higher levelled building. Higher levelled building were designed to\r\n            look more complex to make the player feel the progression they made\r\n            when upgrading buildings.\r\n          </p>\r\n          <p>\r\n            These complexity of course, runs against the aforementioned low-poly\r\n            style. We decided to use SimCity as example for higher levelled\r\n            building while slightly compensating the complexity for a more\r\n            casual low-poly style.\r\n          </p>\r\n          <img src={ComparisonIllustration} alt=\"Comparison\" className=\"p-5\" />\r\n        </div>\r\n        <div>\r\n          <h2 className=\"text-3xl font-bold md:mb-14\">\r\n            <span className=\"highlight\">Result</span>\r\n          </h2>\r\n          <p>\r\n            Over the course of three months, I created approximately 65 assets\r\n            for the game, each crafted with shading, texturing and lighting\r\n            effects. Additionally, I've also created six character portraits\r\n            intended for in-game uses.\r\n          </p>\r\n          <p>\r\n            However, as the hype surrounding NFTs and crypto dwindled down, this\r\n            project was sadly suspended indefinitely. Nevertheless, as my first\r\n            project I am grateful for the experience gained and the opportunity\r\n            to enhance my 3D skills through my involvement in the Alterland\r\n            project.\r\n          </p>\r\n          \r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n",{"ruleId":"100","severity":1,"message":"101","line":3,"column":8,"nodeType":"102","messageId":"103","endLine":3,"endColumn":15},{"ruleId":"104","severity":1,"message":"105","line":17,"column":17,"nodeType":"106","endLine":17,"endColumn":49},{"ruleId":"104","severity":1,"message":"105","line":18,"column":17,"nodeType":"106","endLine":18,"endColumn":49},{"ruleId":"104","severity":1,"message":"105","line":19,"column":17,"nodeType":"106","endLine":19,"endColumn":49},{"ruleId":"104","severity":1,"message":"105","line":20,"column":17,"nodeType":"106","endLine":20,"endColumn":49},{"ruleId":"100","severity":1,"message":"107","line":3,"column":10,"nodeType":"102","messageId":"103","endLine":3,"endColumn":14},{"ruleId":"100","severity":1,"message":"108","line":2,"column":8,"nodeType":"102","messageId":"103","endLine":2,"endColumn":29},{"ruleId":"100","severity":1,"message":"107","line":3,"column":10,"nodeType":"10